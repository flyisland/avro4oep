<?xml version="1.0" encoding="UTF-8"?>
<cache-config>
    <caching-scheme-mapping>    
        <cache-mapping>
            <cache-name>urCache</cache-name>
            <scheme-name>demo-distributed</scheme-name>
        </cache-mapping>
    </caching-scheme-mapping>  
    
    <caching-schemes>    
        <distributed-scheme>
                <scheme-name>demo-distributed</scheme-name>
                <service-name>demo-service</service-name>
                <thread-count>8</thread-count>			
                <partition-count>509</partition-count>
                <backing-map-scheme>
                        <local-scheme>
                                <scheme-ref>backing-map</scheme-ref>
                        </local-scheme>
                </backing-map-scheme>
                <autostart>true</autostart>
        </distributed-scheme>
        
        <local-scheme>
                <scheme-name>backing-map</scheme-name>
        </local-scheme>
    </caching-schemes>
</cache-config>

